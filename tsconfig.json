{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "es2021",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "noEmitOnError": false,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
